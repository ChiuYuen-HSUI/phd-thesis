    number = {434},
    pages = {883--904},
    posted-at = {2011-08-13 03:47:38},
    priority = {2},
    publisher = {American Statistical Association},
    title = {Markov Chain Monte Carlo Convergence Diagnostics: A Comparative Review},
    url = {http://dx.doi.org/10.2307/2291683},
    volume = {91},
    year = {1996}
}
